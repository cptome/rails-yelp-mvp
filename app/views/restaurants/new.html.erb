<h1>Restaurant#new</h1>
<p>Find me in app/views/restaurants/new.html.erb</p>
<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>

  <%= f.label :category %>
  <%= f.number_field :category %>
  <%= f.submit %>
<% end %>
